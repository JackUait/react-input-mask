{
	"name": "react-input-mask",
	"description": "Masked input component for React",
	"version": "3.0.0-alpha.2",
	"homepage": "https://github.com/JackUait/react-input-mask",
	"license": "MIT",
	"author": "Evgeniy Pyatkov <jackuait@gmail.com>",
	"scripts": {
		"clean": "rimraf lib dist",
		"build": "cross-env BABEL_ENV=rollup rollup -c",
		"start": "cross-env BABEL_ENV=test NODE_ENV=development webpack-dev-server",
		"prepare": "npm run lint && npm test && npm run clean && npm run build && husky install",
		"lint": "eslint ./src ./tests ./*.js",
		"lint-fix": "eslint --fix ./src ./tests ./*.js",
		"test": "npm run build && npm run test:input && npm run test:server-render && npm run test:build",
		"test:input": "cross-env NODE_ENV=test BABEL_ENV=test karma start",
		"test:server-render": "cross-env BABEL_ENV=test mocha --require @babel/register ./tests/server-render",
		"test:build": "cross-env BABEL_ENV=test mocha --require @babel/register ./tests/build"
	},
	"keywords": [
		"react",
		"input",
		"mask",
		"masked",
		"react-component"
	],
	"main": "index.js",
	"files": [
		"lib",
		"dist"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/JackUait/react-input-mask"
	},
	"peerDependencies": {
		"react": ">=16.8",
		"react-dom": ">=16.8"
	},
	"devDependencies": {
		"@babel/cli": "^7.16.0",
		"@babel/core": "^7.16.0",
		"@babel/eslint-parser": "^7.16.3",
		"@babel/plugin-proposal-class-properties": "^7.16.0",
		"@babel/plugin-proposal-class-static-block": "^7.16.0",
		"@babel/plugin-transform-modules-commonjs": "^7.16.0",
		"@babel/plugin-transform-proto-to-assign": "^7.16.0",
		"@babel/polyfill": "^7.12.1",
		"@babel/preset-env": "^7.16.4",
		"@babel/preset-react": "^7.16.0",
		"@babel/register": "^7.16.0",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.2.3",
		"babel-plugin-dev-expression": "^0.2.3",
		"babel-plugin-minify-dead-code-elimination": "^0.5.1",
		"babel-plugin-transform-react-remove-prop-types": "^0.4.24",
		"chai": "4.2.0",
		"console-polyfill": "0.3.0",
		"cross-env": "5.2.1",
		"eslint": "8.4.1",
		"eslint-plugin-import": "2.20.0",
		"eslint-plugin-jsx-a11y": "6.5.1",
		"eslint-plugin-optimize-regex": "1.2.1",
		"eslint-plugin-react": "7.27.1",
		"eslint-plugin-react-hooks": "4.3.0",
		"eslint-plugin-sonarjs": "0.11.0",
		"html-webpack-plugin": "3.2.0",
		"husky": "^7.0.0",
		"karma": "4.4.1",
		"karma-chrome-launcher": "3.1.0",
		"karma-mocha": "1.3.0",
		"karma-webpack": "4.0.2",
		"mocha": "6.2.2",
		"prettier": "1.19.1",
		"puppeteer": "1.20.0",
		"react": "16.12.0",
		"react-dom": "16.12.0",
		"rollup": "1.29.0",
		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-commonjs": "10.1.0",
		"rollup-plugin-node-resolve": "5.2.0",
		"rollup-plugin-replace": "2.2.0",
		"rollup-plugin-size-snapshot": "0.10.0",
		"rollup-plugin-terser": "5.2.0",
		"webpack": "4.41.5",
		"webpack-cli": "3.3.10",
		"webpack-dev-server": "3.10.1"
	},
	"dependencies": {
		"invariant": "2.2.4",
		"prop-types": "15.7.2",
		"warning": "4.0.3"
	}
}
